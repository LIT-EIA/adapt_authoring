<h3>{{{ helpDialogTitle }}}</h3>

<nav aria-label="Breadcrumb" class="breadcrumbs">
  <ol class="breadcrumbs-list">
    <li class="item">
      <a href class="next-view-link" data-help-dialog-next-view="TypeOfIssueView">{{{ t 'app.typeofissue' }}}</a>
    </li>
    <li aria-hidden="true" class="separator unselectable">&gt;</li>
    <li class="item">
      <a href class="next-view-link" data-help-dialog-next-view="AccessibilityIssueView">{{{ t 'app.helpdialog.accessibilityissue.link' }}}</a>
    </li>
    <li aria-hidden="true" class="separator unselectable">&gt;</li>
    <li class="item">
      <a href class="next-view-link" data-help-dialog-next-view="AccessibilityToolsAndDocumentationsView">{{{ t 'app.helpdialog.accessibilityissuewithcourse.link' }}}</a>
    </li>
    <li aria-hidden="true" class="separator unselectable">&gt;</li>
    <li class="item">
      <a href class="next-view-link" aria-current="page">{{{ t 'app.representative' }}}</a>
    </li>
  </ol>
</nav>

<p>
  {{{ t 'app.helpdialog.accessibilityissuewithcourse.text' }}}
</p>

<div class="rep-selector-wrapper">
  <label class="sr-only" for="help-dialog-accessibility-rep-selector">{{{ t 'app.selectrepresentativefromaccessibilityalliesnetwork' }}}</label>
  <select id="help-dialog-accessibility-rep-selector" class="business-line-select accessibility-business-line">
    <option></option>
    {{#each properties._businessLines}}
      <option> {{ title }} </option>
    {{/each}}
  </select>
</div>
<p class="reps-texts display-none"><em>{{{ t 'app.heldialog.businessline.representative.helptext' }}}</em></p>
<p class="no-reps-texts display-none"><em>{{{ t 'app.heldialog.businessline.norepresentative' }}}</em></p>

<div class="help-dialog-accessibility-reps"></div>

<p class="support-reps-texts display-none">{{{ td 'app.heldialog.accessibilityissuewithcourse.support.helptext' '{"classname": "next-view-link", "nextview": "ComplianceIssueView"}' }}}</p>
<div>
  <button class="help-dialog-back-button action-primary" data-help-dialog-previous-view="AccessibilityToolsAndDocumentationsView">{{ t 'app.back' }}</button>
</div>