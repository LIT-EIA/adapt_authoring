<!DOCTYPE html>
<html lang="en" data-adapt-framework-version="{{adapt_framework}}" data-adapt-authoring-version="{{adapt_authoring}}">

<head>
  <title>{{productName}}</title>
  <link rel="icon" type="image/png" href="css/assets/favicon.png" />
  <link rel="stylesheet" href="css/adapt.css" />
  <link rel="stylesheet" href="font-awesome-4.7.0/css/font-awesome.min.css">
  <script type="text/javascript" src="modernizr.js"></script>
  <script type="text/javascript" src="ckeditorTranslationFr.js"></script>
  <script type="text/javascript" src="ckeditor.js"></script>
  {{#if isProduction}}
  <script type="text/javascript" src="require.js"
    data-main="js/origin.js{{#if dateStampAsString}}?{{dateStampAsString}}{{/if}}"></script>
  {{else}}
  <script type="text/javascript" src="require.js"></script>
  <script type="text/javascript" src="core/config.js"></script>
  <script type="text/javascript" src="core/app.js"></script>
  {{/if}}
  <meta http-equiv="cache-control" content="no-store, no-cache, must-revalidate" />
  <meta http-equiv="Pragma" content="no-store, no-cache" />
  <meta http-equiv="Expires" content="0" />
  <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1" />
  {{#if useAnalytics}}
  <!-- Google Analytics -->
  <script>
    window.ga = window.ga || function () { (ga.q = ga.q || []).push(arguments) }; ga.l = +new Date;
    ga('create', '{{trackingId}}', 'auto');
    ga('send', 'pageview');
  </script>
  <script async src='//www.google-analytics.com/analytics.js'></script>
  <!-- End Google Analytics -->
  {{/if}}
</head>

<body>
  <nav id="navigation_container"></nav>
  <div id="content_wrapper">
    <div id="sidebar_container"></div>
    <div id="app" class="app container">
      <div class="location-nav">
        <div tabindex="-1" id="mainContent" class="location-title">
        </div>
        <div class="location-options">
        </div>
      </div>
      <div class="app-inner">
      </div>
      <div class="loading">
        <div class="loading-inner">
          <div class="loading-logo">
            <img src="css/assets/adapt-learning-logo-outline.png">
          </div>
          <div class="loading-anim">
            <div class="circle1"></div>
            <div class="circle2"></div>
            <div class="circle3"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>

</html>
